{
    "manifest_version": 2,
    "name": "DoctoChatBot",
    "description": "Chatbot pour Doctolib en extension Chrome",
    "version": "1.0",
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "ChatBot",
        "default_icon": "img/doctolib.png"
    },

    "permissions": ["activeTab", "declarativeContent", "storage", "tabs"]
}
